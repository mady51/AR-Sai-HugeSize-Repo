  <apex:variable var="chartOptions" value="
        chart: {
            borderColor:'',
            height: 400,
            renderTo: 'chart-container',
            type: 'column',
            align: 'center'
        },
        credits: {
            enabled: false
        },
        title: {
            text: null
        },
        plotOptions: {
            series: {
				borderWidth: 0,
                stacking: 'normal'
            }
        },
        yAxis: {
            labels: {
                enabled: true,
                formatter: function() {
                               return '{!currencySymbol}' + this.value;
                           }
            },
            title: {text: null},
            min: 0
        },
        xAxis: {
            reversed: false,
            categories: {!chart.categories}
        },
        tooltip: {
          formatter: function() {
            return 'As of: <b>' + this.x + '</b><br/>' + this.series.name + ': <b>$' +
                this.y.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,') + '</b>';
          }
        },
        legend: {
            margin: 26,
            layout: 'horizontal',
            align: 'center',
            verticalAlign: 'bottom',
            itemWidth: 175,
            width: 880,
            itemMarginTop: 5,
            itemMarginBottom: 5,
            reversed: false
        }"/>

