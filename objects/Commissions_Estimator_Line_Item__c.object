    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>CEL_of_Last_Viewed_CE__c</fullName>
        <externalId>false</externalId>
        <formula>Commissions_Estimator_Parent__c = Left(  $Setup.Last_Viewed_Commission_Estimator__c.Commission_Estimator_Id__c , 15)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CEL of Last Viewed CE</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Category__c</fullName>
        <description>Outputs the product category based on the product type (name) chosen</description>
        <externalId>false</externalId>
        <formula>CASE( DELETE_Product_Type__c , 

&quot;Dental FI HMO&quot;, &quot;Dental&quot;,

&quot;Dental ASO JAA&quot;, &quot;Dental&quot;,

&quot;Vision FI&quot;, &quot;Vision&quot;,

&quot;Vision ASO&quot;, &quot;Vision&quot;,

&quot;Medical&quot;

)</formula>
        <label>Line of Business</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
