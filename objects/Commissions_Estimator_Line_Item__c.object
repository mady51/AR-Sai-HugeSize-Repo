        <precision>5</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Sort_field__c</fullName>
        <description>Automatically sets the value based on the product type (name) picklist.  Will be used to sort in the Commissions Parent related list view per the original Excel</description>
        <externalId>false</externalId>
        <formula>CASE( DELETE_Product_Type__c , 

&quot;Fully Insured (includes Retail Pharm Incentive)&quot;, 1, 
&quot;GRS - Medicare Advantage&quot;, 2,
&quot;Min Prem, ABF&quot;, 3,
&quot;ASO Only (w/out Rx &amp; Stop Loss; embedded UM/CM)&quot;, 4,
&quot;Student Health - Fully Insured&quot;, 5,
&quot;N/W Rental/JAA (JAA L&amp;T and Hospital only)&quot;, 6,
&quot;Rx Drug - ASO&quot;, 7,
&quot;Specific Stop Loss&quot;, 8,
&quot;Student Health - ASO &amp; JAA&quot;, 9,
&quot;GRS - Medicare Part D&quot;, 10,
&quot;UM/CM for JAA&quot;, 11,
&quot;JAA Commercial only&quot;, 12,
&quot;Aggregate SL - ASO&quot;, 13,
&quot;Dental FI/HMO&quot;, 14,
&quot;Dental ASO/JAA&quot;, 15,
&quot;Vision FI&quot;, 16,
17
)</formula>
        <label>Sort field</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TestFormula__c</fullName>
        <externalId>false</externalId>
        <formula>if( AND(Commissions_Estimator_Parent__r.Medical__c = TRUE,  Category__c = &quot;medical&quot;), TRUE, FALSE)</formula>
