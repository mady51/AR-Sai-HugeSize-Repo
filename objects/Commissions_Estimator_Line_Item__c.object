    <validationRules>
        <fullName>Allow_only_FI_and_ASO_goals</fullName>
        <active>false</active>
        <description>Users can only enter goals for Fully Insured and ASO</description>
        <errorConditionFormula>AND(


NOT(ISBLANK(Goal__c)),

NOT(

OR(
ISPICKVAL(DELETE_Product_Type__c , &quot;Fully Insured (includes Retail Pharm Incentive)&quot;),

ISPICKVAL(DELETE_Product_Type__c , &quot;ASO Only (w/out Rx &amp; Stop Loss; embedded UM/CM)&quot;),

ISPICKVAL(DELETE_Product_Type__c , &quot;Dental FI/HMO&quot;),

ISPICKVAL(DELETE_Product_Type__c , &quot;Dental ASO/JAA&quot;),

ISPICKVAL(DELETE_Product_Type__c , &quot;Vision FI&quot;),

ISPICKVAL(DELETE_Product_Type__c , &quot;Vision ASO&quot;)
))

)</errorConditionFormula>
        <errorMessage>You only need to input goals for Fully Insured and ASO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_equal_zero</fullName>
        <active>true</active>
        <description>This is the denominator in a division problem and cannot equal zero</description>
        <errorConditionFormula>Goal__c = 0</errorConditionFormula>
        <errorDisplayField>Goal__c</errorDisplayField>
        <errorMessage>The goal cannot equal zero.  Please enter a valid number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_enter_FI_ASO_goals</fullName>
        <active>true</active>
        <description>Some product types require a goal</description>
        <errorConditionFormula>AND(

ISBLANK (Goal__c ),

NOT(ISBLANK( Total_Members__c )),

 Goal_Eligible__c 

)</errorConditionFormula>
        <errorMessage>You must enter a goal for Fully Insured and ASO line items</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
