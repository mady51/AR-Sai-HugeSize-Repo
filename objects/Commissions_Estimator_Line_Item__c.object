        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Goal__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the target (goal) number of members for this line item</inlineHelpText>
        <label>Goal</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
