        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You cannot edit the Assigned To field on LinkedIn Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_status_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <errorConditionFormula>OR(RecordType.Name = &quot;inMail&quot;, RecordType.Name = &quot;Messages&quot;, RecordType.Name = &quot;PointDrive Presentation Created&quot;, RecordType.Name = &quot;PointDrive Presentation Viewed&quot;, RecordType.Name = &quot;LinkedIn_Call&quot;)
&amp;&amp; (ISCHANGED( Status ))</errorConditionFormula>
