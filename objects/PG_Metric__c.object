    <validationRules>
        <fullName>PGMETR_VR01_Status_Denied_Add_Comment</fullName>
        <active>true</active>
        <description>Prevent a user who tries to update Status=Denied and the Approval Comments field is null</description>
        <errorConditionFormula>AND
(
 ISPICKVAL( Status__c , &quot;Denied&quot;) ,
 ISBLANK( Approval_Comments__c ) 
)</errorConditionFormula>
        <errorMessage>Need to specify Comments when Status is &apos;Denied&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PGMETR_VR02_PGApprover_Null</fullName>
        <active>true</active>
        <description>Prevents a user who tries to update Status to &quot;Approved&quot; and the PG Approver field is null</description>
        <errorConditionFormula>AND
(
 ISPICKVAL( Status__c , &quot;Approved&quot; ) ,
 ISBLANK(  PG_Approver__c  ) 
)</errorConditionFormula>
        <errorMessage>PG Approver cannot be null when Status is &apos;Approved&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PGMETR_VR03_Catalog_Item_Cannot_Change</fullName>
        <active>true</active>
        <description>Prevents a user from trying to update the &apos;PG Catalog Item&apos; lookup field</description>
        <errorConditionFormula>AND
(
  NOT(ISNEW()),
 ISCHANGED( PG_Catalog_Item__c ) 
)</errorConditionFormula>
        <errorDisplayField>PG_Catalog_Item__c</errorDisplayField>
        <errorMessage>This field may not be changed. If a different PG is appropriate, update Status to &apos;Denied&apos; or &apos;Withdrawn&apos; and create a new PG Metric record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PGMETR_VR05_Net_Metric_Requires_PGReport</fullName>
        <active>true</active>
        <description>&apos;PG Metric&apos; has (a) Record Type=New Metric Request AND (b) PG Reporting Team Status does not equal &apos;Approved&apos;, the PG Metric record cannot be marked &apos;Approved for Catalog Addition&apos;</description>
        <errorConditionFormula>IF
(
CONTAINS(RecordType.Name, &quot;New Metric Request&quot;) &amp;&amp; NOT(ISPICKVAL(PG_Reporting_Team_Status__c, &apos;Approved&apos;)) &amp;&amp;
ISPICKVAL( Status__c , &apos;Approved for Catalog Addition&apos;), 
true, 
false
)</errorConditionFormula>
        <errorMessage>New Metric Requests must be Approved by PG Reporting Team before they can be given &apos;Status=Approved for Catalog Addition&apos;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
