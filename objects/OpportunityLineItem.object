        <fullName>New_or_Upsell_Amount__c</fullName>
        <description>Set amount for renewal opportunities as zero for Forecasting new or upsell amounts only.</description>
        <externalId>false</externalId>
        <formula>IF(   Is_Renewal_Opportunity__c = False ,
if( Is_this_PS__c = false, (Quantity* UnitPrice* of_Months__c),Quantity* UnitPrice), 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>New or Upsell Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Currency</type>
    </fields>
