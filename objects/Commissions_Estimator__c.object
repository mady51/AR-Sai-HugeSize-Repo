    <fields>
        <fullName>Dental_Kicker__c</fullName>
        <externalId>false</externalId>
        <formula>If(Total_Dental_Base_Estimate__c = 0  ||  FI_ASO_GTA_Dental__c = 0, 0,

Total_Dental_Base_Estimate__c /  FI_ASO_GTA_Dental__c *

If(FI_ASO_GTA_Dental__c &lt;= 0.8, 0,

If(FI_ASO_GTA_Dental__c &lt;= 1.15, (FI_ASO_GTA_Dental__c  - 0.8)*0.5,

IF(FI_ASO_GTA_Dental__c &lt;= 1.5, 0.35*0.5 + (FI_ASO_GTA_Dental__c  - 1.15)*1,

(0.35*0.5)+(0.35*1)+((FI_ASO_GTA_Dental__c -1.5)*1.5))
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Dental Kicker</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
