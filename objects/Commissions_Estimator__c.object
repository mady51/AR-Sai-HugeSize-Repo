    <fields>
        <fullName>Total_Vision_Commissions__c</fullName>
        <externalId>false</externalId>
        <formula>Total_Vision_Base_Estimate__c +  Vision_Kicker__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Vision Commissions</label>
        <precision>18</precision>
        <required>false</required>
