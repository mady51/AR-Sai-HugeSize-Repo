    <fields>
        <fullName>Dental_Goal__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter a total goal for all dental products</inlineHelpText>
        <label>Dental Goal</label>
        <precision>18</precision>
        <required>false</required>
