    </fields>
    <fields>
        <fullName>Vision_Goal__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter a total goal for all vision products</inlineHelpText>
        <label>Vision Goal</label>
        <precision>18</precision>
        <required>false</required>
