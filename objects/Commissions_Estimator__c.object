    <fields>
        <fullName>Base_Payment__c</fullName>
        <description>Rolls up all commissions line item estimated payments to a total estimated commission</description>
        <externalId>false</externalId>
        <inlineHelpText>This is the base payment for each line item but does not account for kickers at the individual product level</inlineHelpText>
        <label>Base Payment (Excl Kickers)</label>
