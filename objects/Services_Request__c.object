    <validationRules>
        <fullName>Mandate_High_Urgency_Reason</fullName>
        <active>true</active>
        <description>High Urgency Reason will be required when the &apos;Urgency Level&apos; is selected as &apos;High&apos;.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Urgency_Level__c , &apos;High&apos;),
ISBLANK(High_Urgency_Reason__c) 
)</errorConditionFormula>
        <errorMessage>Please enter the &apos;High Urgency Reason&apos; to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Services_Hours_and_Price_Fields</fullName>
        <active>true</active>
        <description>Mandate Service Details (ES Team to Update)
Service Hours
List Services Price
Discounted Services Price</description>
        <errorConditionFormula>AND ( ISPICKVAL( SR_Status__c , &apos;3 - Provided to Sales&apos;) ,  
OR(
ISBLANK( Service_Hours__c ),
ISBLANK(List_Service_Price__c) , 
ISBLANK( Discounted_Services_Price__c)
)
)</errorConditionFormula>
        <errorMessage>&apos;Service Hours&apos;, &apos;List Service Price&apos;, and &apos;Discounted Services Price&apos; are required when &apos;SR Status&apos; is &apos;Provided to Sales&apos;. Please enter all the required fields to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>To_Mandate_Number_of_VPN_Connections</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( VPN_Connectivity__c, &apos;Yes&apos;),   ISBLANK(Number_of_VPN_Connections__c)
)</errorConditionFormula>
        <errorMessage>Please enter &apos;Number of VPN Connections&apos; since you have choosen &apos;VPN Connectivity&apos; as &apos;Yes&apos;.</errorMessage>
    </validationRules>
